{
  "name": "nextclick-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "node scripts/cleanup-pino.js",
    "dev": "node scripts/dev.js",
    "dev:simple": "next dev",
    "dev:next-only": "next dev 2>&1 | npx pino-pretty --colorize --translateTime 'SYS:HH:MM:ss' --ignore pid,hostname",
    "dev:tsc": "tsc --noEmit --watch --preserveWatchOutput",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:list": "playwright test --reporter=list 2>&1 | tee src/__test-results__/playwright-test-output.log"
  },
  "dependencies": {
