    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "humps": "^2.0.1",
    "lucide-react": "^0.554.0",
    "nanoid": "^5.1.6",
    "next": "16.0.3",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
